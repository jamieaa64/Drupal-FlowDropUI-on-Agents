{
    "name": "drupal/flowdrop",
    "type": "drupal-module",
    "description": "A visual workflow editor for Drupal",
    "keywords": [
        "Drupal"
    ],
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/flowdrop",
    "support": {
        "issues": "https://www.drupal.org/project/issues/flowdrop",
        "source": "https://git.drupalcode.org/project/flowdrop"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=8.2",
        "drupal/core": "^11.2"
    },
    "require-dev": {
        "drush/drush": "^13.0",
        "drupal/modeler_api": "^1.0",
        "drupal/bpmn_io": "^3.0",
        "drupal/mcp": "^1.0",
        "drupal/coffee": "^2.0",
        "drupal/ultimate_cron": "^2.0@beta",
        "drupal/queue_ui": "^3.2",
        "drupal/eca": "^3.0",
        "drupal/ai": "^1.1",
        "drupal/ai_agents": "^1.1",
        "drupal/simple_oauth": "^6.0",
        "drupal/cors_ui": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\flowdrop\\": "src/"
        }
    }
}